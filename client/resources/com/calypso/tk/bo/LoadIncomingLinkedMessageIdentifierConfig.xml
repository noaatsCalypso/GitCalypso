        <description>Prepared Statement Query to load Incoming Linked Message Object Identifier Configuration
        </description>
        <idColumn>incoming_linked_msg_identifier.config_id</idColumn>
        <from>incoming_linked_msg_identifier</from>
        <where><![CDATA[1=1]]></where>
        <OptionalCondition name="template" paramCount="1">
