<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<database xmlns="http://www.calypso.com/schemer">

	<table name="domain_values" xmlns="">
		<row cat="collateral-reconciliation-engine">
			<column name="name">engineName</column>
			<column name="value">ReconciliationEngine</column>
			<column name="description"></column>
		</row>
		<row cat="collateral-reconciliation-engine">
			<column name="name">applicationName</column>
			<column name="value">ReconciliationEngine</column>
			<column name="description">ReconciliationEngine</column>
		</row>
	</table>
	
	<table name="engine_config" xmlns="">
		<row cat="collateral-reconciliation-engine">
			<column name="engine_id">101</column>
			<column name="engine_name">ReconciliationEngine</column>
			<column name="engine_comment"></column>
		</row>
	</table>

	<table name="engine_param" xmlns="">
		<row cat="collateral-reconciliation-engine">
			<column name="engine_name">ReconciliationEngine</column>
			<column name="param_name">CLASS_NAME</column>
			<column name="param_value">com.calypso.engine.collateral.reconciliation.ReconciliationEngine</column>
		</row>
		<row cat="collateral-reconciliation-engine">
			<column name="engine_name">ReconciliationEngine</column>
			<column name="param_name">DISPLAY_NAME</column>
			<column name="param_value">Reconciliation Engine</column>
		</row>
		<row cat="collateral-reconciliation-engine">
			<column name="engine_name">ReconciliationEngine</column>
			<column name="param_name">INSTANCE_NAME</column>
			<column name="param_value">engineserver</column>
		</row>
	</table>

	<table name="ps_event_config" xmlns="">
		<row cat="collateral-reconciliation-engine">
			<column name="event_config_name">Back-Office</column>
			<column name="event_class">PSEventMessage</column>
			<column name="engine_name">ReconciliationEngine</column>
		</row>
	</table>

	<table name="ps_event_filter" xmlns="">
		<row cat="collateral-reconciliation-engine">
			<column name="event_config_name">Back-Office</column>
			<column name="engine_name">ReconciliationEngine</column>
			<column name="event_filter">ReconciliationEventFilter</column>
		</row>
	</table>

</database>
