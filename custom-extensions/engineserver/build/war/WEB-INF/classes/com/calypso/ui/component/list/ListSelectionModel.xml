<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
  	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- Constants from ListSelectionModel -->

	<bean id="javax.swing.ListSelectionModel.SINGLE_SELECTION"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.ListSelectionModel.SINGLE_INTERVAL_SELECTION"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.ListSelectionModel.MULTIPLE_INTERVAL_SELECTION"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />

</beans>
