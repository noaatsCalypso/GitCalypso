<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
  	http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- Constants from JTable -->

	<bean id="javax.swing.JTable.AUTO_RESIZE_OFF"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.JTable.AUTO_RESIZE_NEXT_COLUMN"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.JTable.AUTO_RESIZE_SUBSEQUENT_COLUMNS"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.JTable.AUTO_RESIZE_LAST_COLUMN"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
	<bean id="javax.swing.JTable.AUTO_RESIZE_ALL_COLUMNS"
		class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />

</beans>
